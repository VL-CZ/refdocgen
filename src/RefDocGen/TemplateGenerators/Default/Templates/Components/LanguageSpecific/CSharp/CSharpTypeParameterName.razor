@inherits ComponentBase

@code {
    [Parameter]
    public required TypeParameterTM TypeParameter { get; set; }
}


@{
    var modifiers = TypeParameter.Modifiers[Language.CSharp];
}

@if (modifiers.Any())
{
    <span class="refdocgen-text-tertiary">
        @string.Join(' ', modifiers)
    </span>
    <span> </span>
}
<span class="refdocgen-text-secondary">
    @TypeParameter.Name
</span>

