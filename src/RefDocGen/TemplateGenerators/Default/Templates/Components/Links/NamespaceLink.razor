@inherits ComponentBase

@code {
    [Parameter]
    public required string Namespace { get; set; }
}

@{
    var namespaceUrl = $"{Namespace}.html";
}

<h6>
    <samp data-id="type-namespace">
        <a href="@namespaceUrl" class="refdocgen-link-secondary">
            <span class="text-primary">namespace</span>
            <span class="text-secondary">@Namespace</span>
        </a>
    </samp>
</h6>
