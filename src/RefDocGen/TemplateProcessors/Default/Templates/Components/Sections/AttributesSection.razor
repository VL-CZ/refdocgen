@using RefDocGen.TemplateProcessors.Shared.TemplateModels.Types
@inherits ComponentBase

@code {
    [Parameter]
    public required IEnumerable<AttributeTM> Attributes { get; set; }
}

@if (Attributes.Any())
{
    <div class="mt-3">
        <div class="lead">Attributes</div>
        <div class="mx-3" data-id="attributes-section">
            <AttributeList Attributes="@Attributes" />
        </div>
    </div>
}

